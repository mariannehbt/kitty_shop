<!DOCTYPE html>
<html>
  <head>
    <title>KittyShop</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

      <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootswatch/4.3.1/lux/bootstrap.min.css" media="screen">
      <link rel="stylesheet" href="app/assets/stylesheets/main.css">

          <!--**********JavaScript **********-->
   

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    
    <%= render 'layouts/header' %>
    <p class="alert"><%= alert %></p>
    
    



      <script src="vendor/assets/javascripts/popper.min.js"></script>
      <script src="vendor/assets/javascripts/jquery-3.3.1.slim.min.js"></script>
      <script src="vendor/assets/javascripts/bootstrap.min.js"></script>
      <%= yield %>
<%= render 'layouts/footer'%>
  </body>



</html>
